													<div class="p-2"><a href="{{path('produits')}}"> <li class="nav-item active col-xs-3">Produits</li></a></div>

													<div class="p-2"><a href="{{path('service')}}"> <li class="nav-item active col-xs-3">Services</li></a></div>

													<div class="p-2"><a href="{{path('help')}}"> <li class="nav-item active col-xs-3">Aide</li></a></div>

													<div class="p-2"> <a href="{{path('about')}}"><li class="nav-item active col-xs-3">A propos</li></a></div>
												{% if app.user and app.user.userRole == 1 %}
												<div class="p-2"> <a href="admin"><li class="nav-item active col-xs-3">Administration</li></a></div>
												{% endif %}
													
                                         	</div>
                                            

                                        </ul>
                                    </div>
                                </nav>
                            </div>
